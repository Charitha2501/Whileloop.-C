#include <stdio.h>

// Function to find the largest number
int largest(int a, int b) {
    if (a > b) {
        return a;
    }
    return b;
}

int main() {
    int num1, num2;

    printf("Enter two numbers: ");
    scanf("%d %d", &num1, &num2);

    printf("The largest number is: %d\n", largest(num1, num2));

    return 0;
}
